{
  "name": "onfido-sdk-ui-app",
  "version": "0.0.2",
  "description": "Sample App for JavaScript SDK view layer for Onfido identity verification",
  "scripts": {
    "predev": "npm run build",
    "dev:webpack": "NODE_ENV=development webpack-dev-server --inline --hot --progress --https --cert cert.pem --key key.pem",
    "dev": "node app/server.js",
    "start": "superstatic build -p ${PORT:-8080} --host 0.0.0.0 --gzip -c '{\"rewrites\": [{\"source\":\"**\",\"destination\":\"index.html\"}],\"headers\":[{\"source\":\"**\",\"headers\":[{\"key\":\"Cache-Control\",\"value\":\"max-age=86400\"}]}]}'",
    "prestart": "npm run build",
    "build": "NODE_ENV=production webpack -p --progress",
    "prebuild": "mkdir -p dist",
    "lint": "eslint {src,test}",
    "link:sdk": "npm link ../onfido-sdk-ui",
    "unlink:sdk": "rm -r ./node_modules/onfido-sdk-ui && npm install"
  },
  "keywords": [
    "onfido",
    "sdk",
    "document",
    "capture",
    "identity",
    "verification",
    "sample"
  ],
  "files": [
    "src"
  ],
  "license": "MIT",
  "author": "Ricardo Freitas <ricardo.freitas@onfido.com> (https://github.com/rfreitas)",
  "devDependencies": {
    "babel-core": "^6.7.2",
    "babel-eslint": "^6.0.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-es2015-modules-umd": "^6.8.0",
    "babel-plugin-transform-react-jsx": "^6.6.5",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-es2015-minimal": "^1.0.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.7.2",
    "babel-runtime": "^6.6.1",
    "core-js": "^2.2.1",
    "cross-env": "^1.0.8",
    "eslint": "^2.5.3",
    "html-webpack-plugin": "^2.14.0",
    "source-map-loader": "^0.1.5",
    "superstatic": "^4.0.2",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "express": "^4.14.0",
    "onfido-sdk-ui": "^0.6.1",
    "pem": "^1.8.3"
  }
}
