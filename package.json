{
  "name": "onfido-sdk-ui-app",
  "version": "0.0.2",
  "description": "Sample App for JavaScript SDK view layer for Onfido identity verification",
  "scripts": {
    "build:vanilla": "NODE_ENV=production webpack -p --progress --config ./src/vanilla/webpack.config.babel.js",
    "prestart:vanilla": "npm run build:vanilla",
    "start:vanilla": "webpack-dev-server --config ./src/vanilla/webpack.config.babel.js --inline --hot --progress --https --port 8090",
    "build:react": "cd src/react && react-scripts build",
    "start:react": "cd src/react && PORT=8020 react-scripts start",
    "lint": "eslint src/**/*.js",
    "link:sdk": "npm link ../onfido-sdk-ui",
    "unlink:sdk": "rm -r ./node_modules/onfido-sdk-ui && npm install"
  },
  "keywords": [
    "onfido",
    "sdk",
    "document",
    "capture",
    "identity",
    "verification",
    "sample"
  ],
  "files": [
    "src"
  ],
  "license": "MIT",
  "author": "Ricardo Freitas <ricardo.freitas@onfido.com> (https://github.com/rfreitas)",
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.4.4",
    "@babel/node": "^7.2.2",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/preset-env": "^7.0.0",
    "@babel/register": "^7.8.6",
    "babel-eslint": "^9.0.0",
    "babel-loader": "^8.0.5",
    "css-loader": "^2.1.1",
    "eslint": "^6.8.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^4.3.0",
    "lint-staged": "^10.5.1",
    "style-loader": "^0.23.1",
    "webpack": "^4.39.2",
    "webpack-cli": "^3.3.7"
  },
  "dependencies": {
    "@babel/runtime-corejs3": "^7.4.4",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "onfido-sdk-ui": "^6.2.0",
    "prettier": "^2.1.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-scripts": "^4.0.0",
    "webpack-dev-server": "^3.11.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "./**/*.(js|jsx|json)": [
      "prettier --write"
    ]
  }
}
